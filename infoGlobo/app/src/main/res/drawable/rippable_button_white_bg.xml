<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@+id/button_touchfeedback_mask" android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/color_orange_FF691E" />
            <corners android:radius="3dp"/>
        </shape>
    </item>

    <item android:id="@+id/button_touchfeedback_background" android:state_pressed="false">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="@color/color_orange_FF691E" />
            <corners android:radius="3dp"/>
            <solid android:color="@color/color_orange_FF691E" />
        </shape>
    </item>

</selector>